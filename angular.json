{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "nestor-website": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "i18n": {
        "sourceLocale": "en-US",
        "locales": {
          "zh": {
            "translation": "src/locale/messages.zh.xlf"
          }
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/nestor-website",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js",
              "@angular/localize/init"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/all.min.css",
              "src/assets/css/swiper-bundle.min.css",
              "src/assets/css/progressbar.css",
              "src/assets/css/meanmenu.min.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/master-branding-agency.css",
              "src/styles.scss"
            ],
            "scripts": [
              "src/assets/js/jquery-3.6.0.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/swiper-bundle.min.js",
              "src/assets/js/counter.js",
              "src/assets/js/progressbar.js",
              "src/assets/js/gsap.min.js",
              "src/assets/js/ScrollSmoother.min.js",
              "src/assets/js/ScrollToPlugin.min.js",
              "src/assets/js/ScrollTrigger.min.js",
              "src/assets/js/SplitText.min.js",
              "src/assets/js/jquery.meanmenu.min.js",
              "src/assets/js/backToTop.js",
              "src/assets/js/main.js",
              "src/assets/js/error-handling.js",
              "src/assets/js/offcanvas.js",
              "src/assets/js/swiper-call.js",
              "src/assets/js/swiper-services.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb",
                  "maximumError": "50kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "nestor-website:build:production"
            },
            "development": {
              "buildTarget": "nestor-website:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "nestor-website:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing",
              "@angular/localize/init"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "313ee709-cfa4-4bbf-ad6c-2e62690acb5f"
  }
}
